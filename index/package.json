{
  "name": "inspiration",
  "version": "1.0.0",
  "description": "hogehoge",
  "repository": "fugafuga",
  "main": "index.js",
  "scripts": {
    "build-css": "node-sass --include-path scss scss/style.scss css/style.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "sync": "browser-sync start --reload-delay 200 --server --files \"./scss/**/*\" \"./*.html\" ",
    "dev": "concurrently --kill-others \"npm run watch-css\" \"npm run sync\""
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "concurrently": "^5.1.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.3"
  },
  "dependencies": {
    "http-proxy": "^1.18.1"
  }
}